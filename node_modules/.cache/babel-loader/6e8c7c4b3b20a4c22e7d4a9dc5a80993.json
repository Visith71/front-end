   <div class=\"noti-title\">\r\n      <h2>ðŸ”” Notifications</h2>\r\n    </div>\r\n    <table class=\"noti-table\">\r\n    <tr v-for=\"booking in bookings\" :key=\"booking.id\"\r\n    :class=\"[booking.read ? 'read':'unread']\">\r\n      <td @click=\"openBooking(booking)\">\r\n        <div>\r\n          <p>New from:</p>\r\n          <h3>{{ booking.user.first_name }} {{ booking.user.last_name }}</h3>\r\n        </div>\r\n      </td>\r\n      <td @click=\"openBooking(booking)\">\r\n        <div class=\"time\">\r\n          <p>Time in:</p>\r\n          <h3>{{ booking.time_in.substring(0, 5) }} {{ parseInt(booking.time_in.substring(0, 2)) > 11 ? 'PM':'AM' }}</h3>\r\n        </div>\r\n      </td>\r\n      <td @click=\"openBooking(booking)\">\r\n        <div class=\"time\">\r\n          <p>Time out:</p>\r\n          <h3>{{ booking.time_out.substring(0, 5) }} {{ parseInt(booking.time_out.substring(0, 2)) > 11 ? 'PM':'AM' }}</h3>\r\n        </div>\r\n      </td>\r\n      <td @click=\"openBooking(booking)\">\r\n        <p>{{ parseInt(booking.time_out.substring(0, 2)) - parseInt(booking.time_in.substring(0, 2)) }}h</p>\r\n      </td>\r\n    </tr>\r\n    </table>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport { format } from 'date-fns'\r\n\r\nexport default {\r\n  props: ['bookings'],\r\n  setup() {\r\n    return {\r\n      format\r\n    }\r\n  },\r\n  data() {\r\n    return {\r\n    }\r\n  },\r\n  methods: {\r\n    async updateBooking(booking) {\r\n    const axiosConfig = {\r\n      headers: {\r\n        'x-access-token': localStorage.getItem('accessToken')\r\n      }\r\n    }\r\n      await axios.put('booking-details/' + booking.id, booking, axiosConfig)\r\n    }\r\n  },\r\n  computed: {\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.noti-title {\r\n  padding: 1em 1em;\r\n}\r\n.main-notification {\r\n  background: #fff;\r\n  width: auto;\r\n  border-radius: 20px;\r\n  z-index: 100;\r\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.2);\r\n  padding-bottom: 2em;\r\n\r\n}\r\ntd {\r\n  border: none;\r\n  padding: .5em .8em;\r\n}\r\ntr h3 {\r\n  font-weight: 500;\r\n  font-size: 1em;\r\n}\r\ntr p {\r\n  font-size: .8em;\r\n}\r\ntr:hover {\r\n  border-bottom: 1px solid var(--black);\r\n}\r\ntr:nth-child(odd) {\r\n  background: #fff;\r\n  transition: .3s cubic-bezier(0.175, 0.885, 0.32, 1.275);\r\n}\r\n.paginator {\r\n  width: 80%;\r\n  font-size: 1em;\r\n}\r\ntable tr.unread {\r\n  border-left: 3px solid var(--black);\r\n  background: #fbfbfb;\r\n}\r\n.read {\r\n  border-bottom: .5px solid rgba(0, 0, 0, 0.1);\r\n}\r\n.unread h3,\r\n.unread p {\r\n  font-weight: 700;\r\n}\r\n</style>"],"sourceRoot":""}]}