=\"preserve\">\r\n        <g>\r\n          <g>\r\n            <path d=\"M279.99,0C125.601,0,0,125.601,0,279.99c0,154.39,125.601,279.99,279.99,279.99c154.39,0,279.99-125.601,279.99-279.99\r\n              C559.98,125.601,434.38,0,279.99,0z M279.99,498.78c-120.644,0-218.79-98.146-218.79-218.79\r\n              c0-120.638,98.146-218.79,218.79-218.79s218.79,98.152,218.79,218.79C498.78,400.634,400.634,498.78,279.99,498.78z\"/>\r\n            <path d=\"M304.226,280.326V162.976c0-13.103-10.618-23.721-23.716-23.721c-13.102,0-23.721,10.618-23.721,23.721v124.928\r\n              c0,0.373,0.092,0.723,0.11,1.096c-0.312,6.45,1.91,12.999,6.836,17.926l88.343,88.336c9.266,9.266,24.284,9.266,33.543,0\r\n              c9.26-9.266,9.266-24.284,0-33.544L304.226,280.326z\"/>\r\n          </g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        <g>\r\n        </g>\r\n        </svg>\r\n        <h1>Week Part</h1>\r\n      </div>\r\n      <div class=\"add-new-button add-user-button\">\r\n        <button>\r\n          <router-link :to=\"{ name: 'TimePart' }\">\r\n            Time Part üïõ\r\n          </router-link>\r\n        </button>\r\n        <button>\r\n          <router-link :to=\"{ name: 'DayPart' }\">\r\n            Day Part üìÖ\r\n          </router-link>\r\n        </button>\r\n        <button @click=\"openAddModal\">\r\n          Create Week-Part ‚ûï\r\n        </button>\r\n      </div>\r\n    </div>\r\n    <div class=\"card-section time-pard-card animate__animated animate__fadeInUp\">\r\n      <div class=\"cards-layout\">\r\n        <div v-if=\"weekPart.items\">\r\n          <WeekPartTable \r\n            :weekPart=\"weekPart\" \r\n            @dispatchWeekPartByPag=\"dispatchWeekPartByPag\"\r\n            @openEditModal=\"openEditModal\"\r\n          />\r\n          <div class=\"table-pagination\">\r\n            <div class=\"page-size\">\r\n              <select v-model=\"itemSize\" class=\"item-size-select\">\r\n                <option v-for=\"opt in itemSizeList\" :key=\"opt\" :value=\"opt\">\r\n                  {{ opt }}\r\n                </option>\r\n              </select>\r\n              <p class=\"item-per-page\"> | Showing {{ getFirstItemNum }} to {{getLastItemNum}} of <i>{{weekPart.total}} items</i></p>\r\n            </div>\r\n            <v-pagination \r\n              v-model=\"pageNum\"\r\n              :pages=\"getTotalPage\"\r\n              :range-size=\"1\"\r\n              active-color=\"f2f2f2\"\r\n            />\r\n          </div>\r\n        </div>\r\n        <div v-else>\r\n          <h1>No Week Part Data Available üåüüôåüëè</h1>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n\r\n  <ModalView v-if=\"openedEditWeekPart\" @closeModal=\"openedEditWeekPart = null\">\r\n    <EditWeekPart \r\n      @changeEditWeekPart=\"changeEditWeekPart\" \r\n      @closeEditWeekPart=\"closeEditWeekPart\" \r\n      :item=\"selectedItem\"\r\n    />\r\n  </ModalView>\r\n\r\n  <ModalView v-if=\"openedAddWeekPart\" @closeModal=\"openedAddWeekPart = null\">\r\n    <AddWeekPart  \r\n      @closeAddWeekPart=\"closeAddWeekPart\"\r\n    />\r\n  </ModalView>\r\n</template>\r\n\r\n<script>\r\nimport VPagination from '@hennge/vue3-pagination';\r\nimport \"@hennge/vue3-pagination/dist/vue3-pagination.css\"\r\nimport WeekPartTable from '../../../components/datatable/weekPartTable.vue'\r\nimport ModalView from '../../../components/main-component/ModalView.vue'\r\nimport EditWeekPart from '../../../components/time-adjustment/EditWeekPart.vue'\r\nimport AddWeekPart from '../../../components/time-adjustment/AddWeekPart.vue'\r\n\r\n\r\nexport default {\r\n  components: {\r\n    VPagination,\r\n    WeekPartTable,\r\n    ModalView,\r\n    EditWeekPart,\r\n    AddWeekPart\r\n  },\r\n  data() {\r\n    return {\r\n      pageNum: 1,\r\n      pageSize: 3,\r\n      itemSize: null,\r\n      itemSizeList: [5, 10, 20, 30, 40, 50],\r\n      pickedItemSize: null,\r\n      selectedItem: null,\r\n      openedEditWeekPart: null,\r\n      openedAddWeekPart: null\r\n    }\r\n  },\r\n  watch: {\r\n    pageNum() {\r\n      let params = {\r\n        page: this.pageNum,\r\n        itemsPerPage: this.itemSize,\r\n        // sortBy: ['first_name'],\r\n        // sortDesc: [false]\r\n      }\r\n      localStorage.setItem('weekPartPagInfo', JSON.stringify(params))\r\n      this.$store.dispatch('timeAdj/getWeekPart', params)\r\n    },\r\n    itemSize(curValue) {\r\n        let params = {\r\n          page: this.pageNum,\r\n          itemsPerPage: this.itemSize,\r\n          // sortBy: ['first_name'],\r\n          // sortDesc: [false]\r\n        }\r\n        localStorage.setItem('weekPartPagInfo', JSON.stringify(params))\r\n        this.$store.dispatch('timeAdj/getWeekPart', params)\r\n    }\r\n  },\r\n  methods: {\r\n    dispatchWeekPartByPag(params) {\r\n      this.$store.dispatch('timeAdj/getWeekPart', {\r\n        itemsPerPage: this.itemSize,\r\n        page: this.pageNum,\r\n        sortDesc: params.sortDesc,\r\n        sortBy: params.sortBy\r\n      })\r\n    },\r\n    changeEditWeekPart({closeModal}) {\r\n      if(closeModal) {\r\n        this.openedEditWeekPart = null\r\n      }\r\n    },\r\n    closeEditWeekPart() {\r\n      this.openedEditWeekPart = null\r\n    },\r\n    openEditModal(item) {\r\n      this.selectedItem = item\r\n      this.openedEditWeekPart = true\r\n    },\r\n    closeAddWeekPart() {\r\n      this.openedAddWeekPart = null\r\n    },\r\n    openAddModal() {\r\n      this.openedAddWeekPart = true\r\n    }\r\n  },\r\n  computed: {\r\n    weekPart() {\r\n      return this.$store.getters['timeAdj/getWeekPart']\r\n    },\r\n    getTotalPage() {\r\n      return Math.ceil(this.weekPart.total / this.itemSize)\r\n    },\r\n    getFirstItemNum() {\r\n      return (this.itemSize * this.pageNum) - (this.itemSize) + 1\r\n    },\r\n    getLastItemNum() {\r\n      const lastItemNum = (this.itemSize * this.pageNum)\r\n      return lastItemNum >= this.weekPart.total ? this.weekPart.total : lastItemNum  \r\n    }\r\n  },\r\n  created() {\r\n    const weekPartPagInfo = JSON.parse(localStorage.getItem('weekPartPagInfo'))\r\n    if (weekPartPagInfo) {\r\n      this.itemSize = weekPartPagInfo.itemsPerPage;\r\n      this.pickedItemSize = weekPartPagInfo.itemsPerPage;\r\n    } else {\r\n      this.itemSize = 10;\r\n      this.pickedItemSize = 10;\r\n    }\r\n\r\n    let params = {\r\n      itemsPerPage: this.itemSize,\r\n      page: this.pageNum,\r\n      // sortBy: ['id'],\r\n      // sortDesc: [false]\r\n    }\r\n    localStorage.setItem('weekPartPagInfo', JSON.stringify(params))\r\n    this.$store.dispatch('timeAdj/getWeekPart', params)\r\n  },\r\n}\r\n</script>\r\n\r\n<style>\r\n.time-pard-card {\r\n  width: 95%;\r\n  margin: 0 auto;\r\n}\r\n</style>"]}]}