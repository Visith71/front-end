n}\r\n</script>\r\n\r\n<style scoped>\r\n.noti-title {\r\n  padding: 1em 1em;\r\n}\r\n.main-notification {\r\n  background: #fff;\r\n  width: auto;\r\n  border-radius: 20px;\r\n  z-index: 100;\r\n  box-shadow: 0px 4px 10px rgba(0, 0, 0, 0.2);\r\n  padding-bottom: 2em;\r\n\r\n}\r\ntd {\r\n  border: none;\r\n  padding: .5em .8em;\r\n}\r\ntr h3 {\r\n  font-weight: 500;\r\n  font-size: 1em;\r\n}\r\ntr p {\r\n  font-size: .8em;\r\n}\r\ntr:hover {\r\n  border-bottom: 1px solid var(--black);\r\n}\r\ntr:nth-child(odd) {\r\n  background: #fff;\r\n  transition: .3s cubic-bezier(0.175, 0.885, 0.32, 1.275);\r\n}\r\n.paginator {\r\n  width: 80%;\r\n  font-size: 1em;\r\n}\r\ntable tr.unread {\r\n  border-left: 3px solid var(--black);\r\n  background: #fbfbfb;\r\n}\r\n.read {\r\n  border-bottom: .5px solid rgba(0, 0, 0, 0.1);\r\n}\r\n.unread h3,\r\n.unread p {\r\n  font-weight: 700;\r\n}\r\n</style>"]}]}